{
  "name": "transcript-proxy",
  "version": "0.1.0",
  "private": true,
  "description": "Load transcript efficiently for the React client app",
  "main": "src/index.js",
  "scripts": {
    "start": "yarn dev",
    "dev": "nodemon src/. --exec babel-node",
    "prebuild": "yarn clean",
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "yarn lint && yarn test"
  },
  "author": "Tahseen Malik <hello@tahseen.me> (https://tahseenmalik.com/)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "axios-mock-adapter": "^1.14.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "jest": "^22.4.2",
    "nodemon": "^1.15.1",
    "rimraf": "^2.6.2"
  }
